{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "./config.json",
        "outDir": "./dist"
      },
      {
        "include": "./routes.json",
        "outDir": "./dist"
      }
    ]
  },
  "generateOptions": {
    "spec": false
  }
}